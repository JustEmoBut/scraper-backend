    "debug:all": "node scripts/debug-all-data.js",
    "debug:mapping": "node scripts/debug-category-mapping.js",
    "fix:mapping": "node scripts/fix-category-mapping.js",
    "restore:all": "node scripts/restore-all-data.js",
